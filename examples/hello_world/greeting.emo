func greet(name: String, age: Int) {
  if _tooOld?(age) {
    print("Oh, ", name, "! ")
    println("you are too old to find a job.")
  } else {
    print("Good morning, ", name)
    println()
  }
}

func _tooOld?(age: Int) -> Bool {
  return _gt?(age, 35)
}

func _gt?(a: Int, b: Int) -> Bool { // Swift
  return a > b
}

func age() -> Int {
  return 22
}

let age = func() -> Int { return 22 }

let name = "David"
var full_name = name + " Lee"

greet(full_name, age())
